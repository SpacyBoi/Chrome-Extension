{
  "name": "Main and Scheduler views",
  "description": "Proof of concept extension to demonstrate switching views in the same windoe (chrome popup)",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "alarms",
    "notifications",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "POC",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "drink_water16.png"
  }
}